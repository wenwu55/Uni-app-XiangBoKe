{
	"easycom": {
		"^u-(.*)": "@/uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [ 
		// 登录
		{
			"path" : "pages/login/login",
			"style" : {}
		},
		// 管理端
		{
			"path": "pages/index/index",
			"style": {
				"navigationStyle": "custom",
				"navigationBarTextStyle": "white"
			}
		},
		// 我的
		{
			"path": "pages/my/index",
			"style": {
				"navigationBarTitleText": "uView-center"
			}
		}
		// 应用中心
        ,{
            "path" : "pages/app/app",
			"style": {
				"navigationStyle": "custom",
				"navigationBarTextStyle": "white"
			}
        }
		//明细
        ,{
            "path" : "pages/mingxi/mingxi",
            "style" : {
				"navigationBarTitleText":"明细"
			}
        }
		// 更多
        ,{
            "path" : "pages/indexMore/indexMore",
            "style" : {
				"navigationBarTitleText":"车场实时统计"
			}
        }
        ,{
            "path" : "pages/tel/tel",
            "style" : {
				"navigationBarTitleText":"支持热线"
			}
        }
    ],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "",
		"navigationBarBackgroundColor": "#fff",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar": {
	    "color": "#909399",
	    "selectedColor": "#5377FF",
	    "borderStyle": "black",
	    "backgroundColor": "#ffffff",
	    "list": [{
	        "pagePath": "pages/index/index",
	        "iconPath": "static/icon/admin.png",
	        "selectedIconPath": "static/icon/admin_select.png",
	        "text": "管理端"
	    },{
	        "pagePath": "pages/app/app",
	        "iconPath": "static/icon/app.png",
	        "selectedIconPath": "static/icon/app_select.png",
	        "text": "应用中心"
	    },{
	        "pagePath": "pages/my/index",
	        "iconPath": "static/icon/my.png",
	        "selectedIconPath": "static/icon/my_select.png",
	        "text": "我的"
	    }]
